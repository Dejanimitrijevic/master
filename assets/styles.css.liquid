}

.faq-sidebar {
  display: none;
}
@media (min-width: 768px) {
  .faq-sidebar {
    width: 200px;
    position: absolute;
    left: calc(50% - 200px - 35px);
    z-index: 1;
    transform: translateX(-50%);
    padding-bottom: 100px;
    padding-top: {{ block_gutters }}px;
    transition: top 0.2s, height 0.2s;
  }
  .faq-sidebar .faq-sidebar__inner {
    position: -webkit-sticky;
    position: sticky;
    top: {{ block_gutters }}px;
    max-height: 90vh;
    overflow: auto;
  }
  .faq-sidebar h3 {
    text-align: left;
    font-size: {{ font_size_base_int }}px;
    margin-bottom: 1em;
  }
  .faq-sidebar ol {
    padding: 0;
    margin: 0;
    list-style-type: none;
  }
  .faq-sidebar ol li {
    margin-bottom: 0.75em;
    padding-right: 20px;
  }
  .faq-sidebar a {
    transition: opacity 0.4s;
    opacity: 0.5;
  }
  .faq-sidebar a.faq-sidebar--active {
    opacity: 1;
  }
  .faq-sidebar a.faq-sidebar--disabled {
    pointer-events: none;
  }
  .faq-sidebar-enabled .faq-sidebar {
    display: inline-block;
  }
  .no-js .faq-sidebar {
    display: none;
  }
}
@media (min-width: 992px) {
  .faq-sidebar {
    width: 220px;
    left: calc(50% - 220px - 45px);
  }
}
@media (min-width: 1200px) {
  .faq-sidebar {
    width: 220px;
    left: calc(50% - 338px);
  }
}

