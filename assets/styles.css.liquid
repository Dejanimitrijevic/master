/* metafields table */
.metafields_table {
  text-align: left;
  margin-bottom: 1em;
  zoom: 1;
  background: {{ col_content_bg }};
  -webkit-border-radius: 2px;
  -ms-border-radius: 2px;
  -moz-border-radius: 2px;
  -o-border-radius: 2px;
  border-radius: 2px;
}

.metafields_table table {
  width: 100%;
}
.metafields_table::before {
  content: "";
  display: table;
}
.metafields_table::after {
  content: "";
  display: table;
  clear: both;
}
.metafields_table th{
  padding: 0.4em 0;
}
.metafields_table th,
.metafields_table tr,
.metafields_table td {
  border: none;
}
.metafields_table td {
  width: 50%;
}
.metafields_table tbody tr:nth-child(odd) {
  background-color: #F7F7F7;
}
.metafields_table tbody tr:nth-child(even) {
  background-color: #FFFFFF;
}
.metafields-table-container{
  margin-top: 1em;
}

